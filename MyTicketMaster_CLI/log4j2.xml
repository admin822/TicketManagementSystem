<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="MyTicketMaster" packages="">
  <Appenders>
    <File name="MyFile" fileName="log4j2.xml">
      <PatternLayout>
        <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %class %m%n"/>
      </PatternLayout>
    </File>
  </Appenders>
  <Loggers>
    <Root level="error">
      <AppenderRef ref="MyFile"/>
    </Root>
  </Loggers>
</Configuration>